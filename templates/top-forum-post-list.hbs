{{! Template from `travis` with MIT license }}
{{! Repo URL: https://github.com/travis-ci/travis-web }}
<LoadingOverlay @visible={{this.isLoading}}>
  {{#each this.topicsToShow as |topic|}}
    <li>
      <ExternalLinkTo @href={{topic.url}}>
        {{topic.title}}
      </ExternalLinkTo>
    </li>
  {{/each}}
</LoadingOverlay>
