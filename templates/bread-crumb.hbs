{{! Template from `ember-crumbly` with MIT license }}
{{! Repo URL: https://github.com/poteto/ember-crumbly }}
{{#if route.linkable}}
  {{#link-to route.path class=linkClass}}
    {{#if hasBlock}}
      {{yield this route}}
    {{else}}
      {{route.title}}
    {{/if}}
  {{/link-to}}
{{else}}
  {{#if hasBlock}}
    {{yield this route}}
  {{else}}
    {{route.title}}
  {{/if}}
{{/if}}
