{{! Template from `discourse` with GPL-2.0-only license }}
{{! Repo URL: git@github.com:discourse/discourse.git }}
{{#each field.choices as |c|}}
  <div class="checkbox-field-choice {{fieldClass}}">
    <label id={{c.id}} value={{c.label}}>
      {{input type="checkbox"
            class="wizard-checkbox"
            click=(action "changed" value="target")
            checked=c.checked}}
      {{#if c.icon}}
        {{d-icon c.icon}}
      {{/if}}
      {{c.label}}
    </label>
  </div>
{{/each}}
